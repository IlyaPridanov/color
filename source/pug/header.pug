-
  const arr = [
    {
      name: "heart",
      width: "22",
      height: "21",
    },
    {
      name: "user",
      width: "20",
      height: "20",
    },
    {
      name: "search",
      width: "22",
      height: "22",
    },
  ]

header.header
  .container.header__row
    a.header__logo(href="#")
      +img("logo","svg",131,26)
    include blocks/nav.pug
    ul.header__icon-list
      each item,index in arr
        li(class="header__icon-item header__icon-item--"+item.name)
          a(href="#")
            +svg(item.name,item.width,item.height)
    button(type="button", data-block="nav").hamburger.header__hamburger.js-btn
      span.hamburger__bar.hamburger__bar--bar1
      span.hamburger__bar.hamburger__bar--bar2
      span.hamburger__bar.hamburger__bar--bar3
      span.hamburger__bar.hamburger__bar--bar4
